{
  "name":"Copy link as markdown only for asana",
  "version":"1.0.1",
  "description": "copy only task name on asana as markdown",
  "content_scripts": [{
    "matches": ["https://app.asana.com/*"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "asana title"
  },
  "permissions": [
   "activeTab", "clipboardRead"
  ],
  "manifest_version":2,
  "icons": {
    "128": "for_asana.png"
  }
}
