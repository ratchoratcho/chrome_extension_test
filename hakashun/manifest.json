{
  "name":"hakashun-test",
  "version":"1.0.0",
  "description": "Chrome Extension Example for manipulating DOM.",
  "content_scripts": [{
    "matches": ["https://app.asana.com/*"],
    "js": ["content.js"]
  }],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "hakashun-test"
  },
  "permissions": [
    "activeTab", "tabs", "clipboardWrite", "clipboardRead"
  ],
  "manifest_version":2
}
